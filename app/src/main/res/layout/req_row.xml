<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="150dp"
    android:layout_marginRight="5dp"
    android:layout_marginLeft="5dp"
    android:layout_marginTop="5dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardBackgroundColor="#FFFFFF">
    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <com.sheatouk.selmy.componentsdonationasu.Util.MyTextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Mahmoud Hesham"
            android:textSize="17sp"
            android:gravity="center"
            android:clickable="true"
            android:textStyle="bold"
            android:id="@+id/req_user"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="2dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginBottom="2dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline6"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="2dp"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toRightOf="@+id/imageView5" />
        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:background="#dad8d8"
            android:id="@+id/view"
            tools:layout_editor_absoluteX="0dp"
            app:layout_constraintTop_toTopOf="@+id/guideline6"
            android:layout_marginTop="0dp" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_action_to"
            android:layout_marginBottom="2dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline6"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="2dp"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:id="@+id/imageView5" />
        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline6"
            app:layout_constraintGuide_percent="0.29"
            android:orientation="horizontal" />
        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline7"
            app:layout_constraintGuide_percent="0.47"
            android:orientation="vertical" />
        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline8"
            app:layout_constraintGuide_percent="0.53"
            android:orientation="vertical" />

        <com.sheatouk.selmy.componentsdonationasu.Util.MyTextView
            android:id="@+id/myTextView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:text="Comp : "
            android:textSize="20sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/req_comp"
            app:layout_constraintLeft_toLeftOf="parent" />
        <com.sheatouk.selmy.componentsdonationasu.Util.MyTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="From : "
            android:textSize="20sp"
            android:layout_marginLeft="5dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@+id/req_from"
            android:id="@+id/myTextView4" />

        <com.sheatouk.selmy.componentsdonationasu.Util.MyTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:text="To : "
            android:textSize="20sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@+id/req_to" />

        <com.sheatouk.selmy.componentsdonationasu.Util.MyTextView
            android:id="@+id/req_comp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="8dp"
            android:text="Arduino Uno"
            android:textSize="20sp"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintLeft_toRightOf="@+id/myTextView3"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guideline6" />

        <com.sheatouk.selmy.componentsdonationasu.Util.MyTextView
            android:id="@+id/req_from"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="8dp"
            android:text="16,Jan,2017"
            android:textSize="20sp"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="@+id/req_comp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/req_comp" />
        <com.sheatouk.selmy.componentsdonationasu.Util.MyTextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="20,Sep,2017"
            android:textSize="20sp"
            android:id="@+id/req_to"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/req_from"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="0dp"
            app:layout_constraintLeft_toLeftOf="@+id/req_from"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginBottom="0dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline9"
            app:layout_constraintVertical_bias="0.0" />

        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline9"
            app:layout_constraintGuide_percent="0.95"
            android:orientation="horizontal" />
    </android.support.constraint.ConstraintLayout>
</android.support.v7.widget.CardView>